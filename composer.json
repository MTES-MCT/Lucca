{
    "name": "numeric-wave/lucca-vs",
    "description": "Cabanisation assistance project and other urban planning offences",
    "type": "project",
    "license": "AGPL-3.0-or-later",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=8.2",
        "ext-gd": "*",
        "ext-intl": "*",
        "ext-pdo": "*",
        "ext-xml": "*",
        "doctrine/doctrine-bundle": "^2.14",
        "doctrine/doctrine-migrations-bundle": "^3.4",
        "firebase/php-jwt": "*",
        "friendsofsymfony/jsrouting-bundle": "3.5.*",
        "knplabs/knp-snappy-bundle": "*",
        "lcobucci/clock": "^2.0",
        "lcobucci/jwt": "^5.5",
        "symfony/asset": "^7.4",
        "symfony/asset-mapper": "^7.4",
        "symfony/cache": "^7.4",
        "symfony/console": "^7.4",
        "symfony/dependency-injection": "^7.4",
        "symfony/dom-crawler": "^7.4",
        "symfony/dotenv": "^7.4",
        "symfony/event-dispatcher": "^7.4",
        "symfony/filesystem": "^7.4",
        "symfony/flex": "^2.2",
        "symfony/form": "^7.4",
        "symfony/framework-bundle": "^7.4",
        "symfony/intl": "^7.4",
        "symfony/mailer": "^7.4",
        "symfony/monolog-bundle": "*",
        "symfony/rate-limiter": "^7.4",
        "symfony/runtime": "^7.4",
        "symfony/security-bundle": "^7.4",
        "symfony/serializer": "^7.4",
        "symfony/stimulus-bundle": "*",
        "symfony/translation": "^7.4",
        "symfony/twig-bundle": "^7.4",
        "symfony/ux-autocomplete": "^2.23",
        "symfony/validator": "^7.4",
        "symfony/yaml": "^7.4",
        "tomsgu/pdf-merger": "*",
        "twig/extra-bundle": "^2.12|^3.0",
        "twig/twig": "^3.0"
    },
    "require-dev": {
        "dama/doctrine-test-bundle": "^8.0.2",
        "doctrine/doctrine-fixtures-bundle": "*",
        "phpunit/phpunit": "^10",
        "symfony/browser-kit": "^7.4",
        "symfony/css-selector": "^7.4",
        "symfony/debug-bundle": "^7.4",
        "symfony/phpunit-bridge": "^7.4",
        "symfony/stopwatch": "^7.4",
        "symfony/web-profiler-bundle": "^7.4"
    },
    "suggest": {
        "ext-apcu": "For best performance of the system caches",
        "symfony/console": "For using the console commands",
        "symfony/yaml": "For using the debug:config and lint:yaml commands",
        "symfony/property-info": "For using the property_info service",
        "symfony/web-link": "For using web links, features such as preloading, prefetching or prerendering"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "symfony/flex": true,
            "symfony/runtime": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "Lucca\\Bundle\\": "src/Lucca/Bundle"
        },
        "exclude-from-classmap": [
            "src/Lucca/*/*/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php82": "*"
    },
    "scripts": {
        "auto-scripts": {
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "cache:clear": "symfony-cmd",
            "importmap:install": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "^7.4",
            "docker": true
        }
    }
}
